{
    "policy" : {
        "name": "my_local_resources_file_types",
        "template": {
            "name": "POLICY_TEMPLATE_NGINX_BASE"
        },
        "applicationLanguage": "utf-8",
        "enforcementMode": "blocking",
        "blocking-settings": {
            "violations": [
                {
                    "name": "VIOL_FILETYPE",
                    "alarm": true,
                    "block": true
                }
            ]
        },
        "filetypeReference": {
            "link": "file:///etc/nginx/includes/nap.d/my-filetypes.txt"
        }
    }
}
